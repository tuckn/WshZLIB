{
  "name": "@tuckn/ZLIB",
  "description": "This module provides helper WSH (Windows Script Host) functions that handle archiver apps (7-Zip and RAR). Usage: [WshZLIB](https://github.com/tuckn/WshZLIB)",
  "version": "2.0.0",
  "author": "Tuckn <tuckn333@gmail.com>",
  "private": false,
  "license": "MIT",
  "homepage": "https://docs.tuckn.net/ZLIB",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuckn/ZLIB.git"
  },
  "bugs": {
    "url": "https://github.com/tuckn/ZLIB/issues"
  },
  "keywords": [
    "Windows",
    "WSH",
    "WSF",
    "JScript",
    "VBScript",
    "module",
    "7-Zip",
    "RAR"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/bundle.js",
  "bin": {},
  "scripts": {
    "install:WshJest": "git submodule add https://github.com/tuckn/WshJest.git ./WshModules/WshJest",
    "install:WshChildProcess": "git submodule add https://github.com/tuckn/WshChildProcess.git ./WshModules/WshChildProcess",
    "installModules": "run-s install:*",
    "updateModules": "git submodule update --remote",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "run-p fix:*",
    "lint:eslint": "eslint --ext js,ts .",
    "lint:prettier": "prettier -l \"**/*.{json,md,yml,yaml}\"",
    "lint:type": "tsc --noEmit",
    "lint": "run-p lint:*",
    "test:name:deflateSync_dryRun_basic": ".\\scripts\\Test ZLIB -t deflateSync_dryRun_basic$",
    "test:name:deflateSync_dryRun_options": ".\\scripts\\Test ZLIB -t deflateSync_dryRun_options$",
    "test:name:getAdaptersPropsSWbemObjs": ".\\scripts\\Test ZLIB -t getAdaptersPropsSWbemObjs$",
    "test:name:getAdaptersPropsObjs": ".\\scripts\\Test ZLIB -t getAdaptersPropsObjs$",
    "test:name:enablesDHCP": ".\\scripts\\Test ZLIB -t enablesDHCP$",
    "test:name:getAdaptersConfsSWbemObjs": ".\\scripts\\Test ZLIB -t getAdaptersConfsSWbemObjs$",
    "test:name:getAdaptersConfsObjs": ".\\scripts\\Test ZLIB -t getAdaptersConfsObjs$",
    "test:name:getIpSetInAdapters": ".\\scripts\\Test ZLIB -t getIpSetInAdapters$",
    "test:name:getDefaultGateways": ".\\scripts\\Test ZLIB -t getDefaultGateways$",
    "test:name:getDnsIPsSetInAdapters": ".\\scripts\\Test ZLIB -t getDnsIPsSetInAdapters$",
    "test:name:exportWinFirewallSettings": ".\\scripts\\Test ZLIB -t exportWinFirewallSettings$",
    "test:name:setIpAddress": ".\\scripts\\Test ZLIB -t setIpAddress$",
    "test:name:setDnsServers": ".\\scripts\\Test ZLIB -t setDnsServers$",
    "test:name:setDnsServersWithWMI": ".\\scripts\\Test ZLIB -t setDnsServersWithWMI$",
    "test:name:importWinFirewallSettings": ".\\scripts\\Test ZLIB -t importWinFirewallSettings$",
    "test:name:shareDirectory_dryRun": ".\\scripts\\Test ZLIB -t shareDirectory_dryRun$",
    "test:ZLIB": ".\\scripts\\Test ZLIB",
    "build": "wsh-packager bundle ./Package.wsf",
    "test:dist:module": ".\\scripts\\Test dist:module",
    "test:dist:bundle": ".\\scripts\\Test dist:bundle",
    "test": "run-s test:dist:*",
    "predocs": "rimraf ./docs",
    "docs:common": "jsdoc --configure ./jsdoc.json",
    "cp-docs-css": "cpy ./assets/styles/jsdoc.css ./docs/styles/",
    "cp-docs-img": "cpy ./assets/img/**/* ./docs/img/",
    "postdocs": "run-p cp-docs-*",
    "docs": "run-s docs:*",
    "up-build-doc": "run-s updateModules build docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo @FIXME \"npx lint-staged\" fixes all .js!"
    }
  },
  "lint-staged": {
    "*.{js}": "eslint --fix",
    "*.{json,css,md,yml,yaml}": "prettier --write"
  },
  "dependencies": {},
  "devDependencies": {
    "@tuckn/insert-gtag": "^1.1.0",
    "@tuckn/wsh-packager": "^1.0.0",
    "@types/activex-scripting": "^1.0.7",
    "@types/windows-script-host": "^5.8.3",
    "cpy-cli": "^3.1.1",
    "docdash": "^1.2.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jsdoc": "^3.6.10",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2"
  }
}
